{
  "title": "mackbook+apple magic keyboard 2",
  "rules": [{
    "description": "caps lock to escape, when with another key to left control",
    "manipulators": [{
      "type": "basic",
      "from": {
        "key_code": "caps_lock",
        "modifiers": {
          "optional": ["left_shift"]
        }
      },
      "to": [{
        "key_code": "left_control"
      }],
      "to_if_alone": [{
        "key_code": "escape"
      }]
    }]
  }, {
    "description": "right command hjkl to left down up right arrows",
    "manipulators": [{
      "type": "basic",
      "from": {
        "key_code": "h",
        "modifiers": {
          "mandatory": ["right_command"]
        }
      },
      "to": [{
        "key_code": "left_arrow"
      }]
    }, {
      "type": "basic",
      "from": {
        "key_code": "j",
        "modifiers": {
          "mandatory": ["right_command"]
        }
      },
      "to": [{
        "key_code": "down_arrow"
      }]
    }, {
      "type": "basic",
      "from": {
        "key_code": "k",
        "modifiers": {
          "mandatory": ["right_command"]
        }
      },
      "to": [{
        "key_code": "up_arrow"
      }]
    }, {
      "type": "basic",
      "from": {
        "key_code": "l",
        "modifiers": {
          "mandatory": ["right_command"]
        }
      },
      "to": [{
        "key_code": "right_arrow"
      }]
    }]
  }, {
    "description": "prevent wrongly command+q",
    "manipulators": [{
      "type": "basic",
      "from": {
        "key_code": "q",
        "modifiers": {
          "mandatory": ["left_command"]
        }
      },
      "parameters": {
        "basic.to_if_held_down_threshold_milliseconds": 512
      },
      "to_if_held_down": [{
        "key_code": "q",
        "modifiers": ["command"],
        "repeat": false
      }]
    }]
  }, {
    "description": "to 40% keyboard",
    "manipulators": [{
      "type": "basic",
      "from": {
        "key_code": "tab",
        "modifiers": {
          "mandatory": ["right_command"],
          "optional": ["left_shift"]
        }
      },
      "to": [{
        "key_code": "grave_accent_and_tilde"
      }]
    }, {
      "type": "basic",
      "from": {
        "key_code": "q",
        "modifiers": {
          "mandatory": ["right_command"],
          "optional": ["left_shift"]
        }
      },
      "to": [{
        "key_code": "1"
      }]
    }, {
